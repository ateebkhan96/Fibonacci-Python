terms = int(input("Enter the number of Terms:\t"))
n1, n2 = 0,1
count=0

if terms <=0:
    print("Enter a positive integer")
elif terms == 1:
    print("Fibonacci Sequence of",terms,".")
    print(n1)
else:
    print("Fibonacci Sequence:")
    while count< terms:
        print(n1)
        n = n1 +n2
        n1=n2
        n2=n
        count+=1
